{
  "name": "@opensom/manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/src/index.ts",
    "lint": "eslint .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "pnpm build && start-test 'pnpm start' http://0.0.0.0:3000/ 'pnpm cypress'",
    "e2e:headless": "pnpm build && start-test 'pnpm start' http://0.0.0.0:3000/ 'pnpm cypress:headless'"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "@types/nodemailer": "^6.4.4",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "nodemailer": "^6.7.3",
    "prisma": "^3.12.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "cypress": "^9.5.4",
    "start-server-and-test": "^1.14.0",
    "ts-node-dev": "^1.1.8"
  }
}
