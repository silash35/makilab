{
  "name": "@opensom/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --exit-child src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "pnpm build && start-test 'pnpm start' http://0.0.0.0:1234/ 'pnpm cypress'",
    "e2e:headless": "pnpm build && start-test 'pnpm start' http://0.0.0.0:1234/ 'pnpm cypress:headless'"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  },
  "dependencies": {
    "@opensom/config": "workspace:*",
    "@prisma/client": "^3.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.36",
    "@types/nodemailer": "^6.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.5",
    "prisma": "^3.14.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "cypress": "^9.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "start-server-and-test": "^1.14.0",
    "ts-node-dev": "^2.0.0"
  }
}
