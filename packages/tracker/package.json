{
  "name": "@opensom/tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"cd ../backend && pnpm dev\" \"next dev\"",
    "build": "next build",
    "build:forTest": "concurrently \"cd ../backend && pnpm build\" \"next build\"",
    "start": "next start",
    "start:forTest": "concurrently -k \"cd ../backend && pnpm start\" \"next start\"",
    "lint": "next lint",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "pnpm build:forTest && start-test 'pnpm start:forTest' http://0.0.0.0:3000/ 'pnpm cypress'",
    "e2e:headless": "pnpm build:forTest && start-test 'pnpm start:forTest' http://0.0.0.0:3000/ 'pnpm cypress:headless'"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "@opensom/config": "workspace:*",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "date-fns": "^2.28.0",
    "next": "^12.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.52.3",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "concurrently": "^7.2.2",
    "cypress": "^10.3.0",
    "start-server-and-test": "^1.14.0"
  }
}
